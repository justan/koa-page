{
  "extends": [
    // "plugin:prettier/recommended",
    "eslint:recommended"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "semi": ["error", "never"],
    "quotes": ["error", "single"]
  },
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  }
}
